{
  "name": "react-resume-template",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 next build --debug",
    "clean": "rm -rf .next node_modules/.cache",
    "compile": "yarn run -T tsc --build --verbose",
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0",
    "start": "next start",
    "sitemap": "yarn next-sitemap",
    "predeploy": "NODE_ENV=production npm run build",
    "deploy": "touch out/.nojekyll && gh-pages -d out --dotfiles"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@types/i18next": "^13.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-i18next": "^8.1.0",
    "autoprefixer": "10.4.16",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.4",
    "i18next": "^24.2.3",
    "next": "13.4.0",
    "nodemailer": "^6.10.0",
    "particles-bg": "^2.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.4.1",
    "ts-pattern": "^5.0.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.3",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/tailwindcss": "^3.1.0",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.46.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sass": "^1.64.2",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "autoprefixer": "10.4.5"
  }
}
